{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "AppHost (Debug)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "cleanup-and-build",
      "program": "${workspaceFolder}/TGHarker.Identity.AppHost/bin/Debug/net10.0/TGHarker.Identity.AppHost.dll",
      "args": [],
      "cwd": "${workspaceFolder}/TGHarker.Identity.AppHost",
      "env": {
        "DOTNET_ENVIRONMENT": "Development",
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "console": "integratedTerminal",
      "stopAtEntry": false
    },
    {
      "name": "Attach to Silo",
      "type": "coreclr",
      "request": "attach",
      "processName": "TGHarker.Identity.Silo"
    },
    {
      "name": "Attach to Web",
      "type": "coreclr",
      "request": "attach",
      "processName": "TGharker.Identity.Web"
    }
  ],
  "compounds": [
    {
      "name": "AppHost + Attach All",
      "configurations": ["AppHost (Debug)", "Attach to Silo", "Attach to Web"],
      "stopAll": true,
      "presentation": {
        "group": "debug",
        "order": 1
      }
    }
  ]
}
